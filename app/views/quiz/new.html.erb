<%= form_for :quiz, url: quiz_path do |f| %>
    <p>
      <%= f.label :question %><br>
      <%= f.text_area :question %>
    </p>

    <p>
      <%= f.label :id %><br>
      <%= f.text_area :id %>
    </p>

    <p>
      <%= f.label :level %><br>
      <%= f.text_area :level %>
    </p>

    <p>
      <%= f.submit %>
    </p>

    <%= link_to 'Back', logs_path %>
<% end %>